<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" tools:context=".MainActivity" tools:deviceIds="wear_round"
    android:background="#68b4ff"
    android:id="@+id/background">

    <TextView android:id="@+id/WitzeXXL" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/WitzeXL"
        android:textSize="20sp"
        android:textColor="@color/black_86p"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="27dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/LOSGEHTS"
        android:id="@+id/buttonstart"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:textColor="@color/dark_red"
        android:textSize="30sp"
        android:onClick="buttonstart"/>


</RelativeLayout>
