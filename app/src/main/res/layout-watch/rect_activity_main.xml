<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:orientation="vertical"
    tools:context=".MainActivity" tools:deviceIds="wear_square"
    android:background="#68b4ff"
    android:id="@+id/background">

    <TextView android:id="@+id/WitzeXXL" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/WitzeXL"
        android:textSize="20sp"
        android:textColor="@color/black_86p"
        android:layout_marginTop="54dp"
        android:layout_gravity="center_horizontal"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="319dp"
        android:layout_alignParentTop="true" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/LOSGEHTS"
        android:id="@+id/buttonstart"
        android:textColor="@color/dark_red"
        android:textSize="30sp"
        android:layout_gravity="center_horizontal"
        android:onClick="buttonstart"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="66dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="102dp" />
</RelativeLayout>
